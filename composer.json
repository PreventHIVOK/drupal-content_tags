{
    "name": "preventhivok/content_tags",
    "description": "Drupal 8 module that adds a Tags vocabulary and field.",
    "type": "drupal-module",
    "version": "1.0.0",
    "keywords": [ "Drupal 8", "module", "taxonomy" ],
    "license": "GPL-2.0+",
    "homepage": "https://github.com/PreventHIVOk/drupal-content_tags#readme",
    "support": {
        "issues": "https://github.com/PreventHIVOk/drupal-content_tags/issues",
        "source": "https://github.com/PreventHIVOK/drupal-content_tags.git"
    },
    "authors": [
        {
            "name": "Curtis Blumer",
            "email": "curtis.blumer@gmail.com",
            "homepage": "http://curtapedia.com",
            "role": "Developer"
        }
    ],
    "replace": {
        "drupal/content_tags": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "require": {
        "php": "^7.0",
        "drupal/core": "^8.0",
        "drupal/taxonomy": "*"
    },
    "scripts": {
        "test": "echo Error: No tests have been specified && exit 1"
    },
    "autoload": { "psr-4": { "Drupal\\content_tags\\": "src" } },
    "autoload-dev": { "psr-4": { "Drupal\\content_tags\\Tests\\": "tests" } },
    "prefer-stable": true,
    "minimum-stability": "dev"
}